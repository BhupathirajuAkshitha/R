data_csv_local<-read.csv("sample_data.csv",header=TRUE)
cat("CSV Data from local file:/n")
print(data_csv_local)

data_txt_local<-read.table("sample_data.txt",header=TRUE,sep="\t")
cat("TXT Data from local file:\n")
print(data_txt_local)

